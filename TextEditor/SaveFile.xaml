<Window x:Class="TextEditor.SaveFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TextEditor"
        mc:Ignorable="d"
        Title="SaveFile" Height="450" Width="350">
    <Grid>
        <ComboBox x:Name="extension" Width="200" Height="50" FontSize="20">
            <TextBlock>.pdf</TextBlock>
            <TextBlock>.txt</TextBlock>
            <TextBlock>.rtf</TextBlock>
        </ComboBox>
        <Button Content="Save" Margin="93,267,92,126" Click="Button_Click"/>
    </Grid>
</Window>
